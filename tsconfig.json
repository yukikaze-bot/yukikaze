{
	"extends": "@sapphire/ts-config",
	"compilerOptions": {
		"baseUrl": ".",
		"rootDir": "src",
		"outDir": "dist",
		"skipLibCheck": true,
		"paths": {
			"@structures/*": ["src/structures/*"],
			"@keys/*": ["src/keys/*"],
			"@utils/*": ["src/utils/*"],
			"#types/*": ["src/types/*"]
		},
		"typeRoots": ["./types"]
	}
}
