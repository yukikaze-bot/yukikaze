{
	"name": "yukikaze",
	"private": true,
	"version": "1.0.0",
	"description": "A multipurpose discord bot,",
	"main": "./dist/index.js",
	"scripts": {
		"build": "rimraf dist && tsc && tsc-alias && copyfiles -u 1 src/languages/**/*.json dist",
		"migrate:dev": "prisma migrate dev",
		"migrate": "prisma migrate deploy && prisma generate",
		"start": "yarn migrate && yarn build && pm2-runtime start dist/index.js --name yukikaze",
		"prettier": "prettier . --write",
		"deploy": "railway up"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/yukikaze-bot/yukikaze.git"
	},
	"keywords": [
		"discord"
	],
	"author": "Yoshida Tomio <https://tomio.codes>",
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/yukikaze-bot/yukikaze/issues"
	},
	"homepage": "https://github.com/yukikaze-bot/yukikaze#readme",
	"dependencies": {
		"@favware/graphql-pokemon": "^5.5.4",
		"@naval-base/ms": "^3.1.0",
		"@prisma/client": "^2.25.0",
		"@sapphire/decorators": "^2.1.3",
		"@sapphire/discord-utilities": "^2.1.3",
		"@sapphire/discord.js-utilities": "^1.5.9",
		"@sapphire/fetch": "^1.0.3",
		"@sapphire/framework": "^1.0.2",
		"@sapphire/pieces": "~1",
		"@sapphire/plugin-i18next": "^1.3.7",
		"@sapphire/plugin-logger": "^1.0.12",
		"@sapphire/plugin-subcommands": "^1.0.5",
		"@sapphire/type": "^2.0.9",
		"@skyra/editable-commands": "^1.0.0",
		"@skyra/saelem": "^3.0.9",
		"apollo-server": "^2.25.1",
		"canvas": "^2.8.0",
		"capitalize": "^2.0.3",
		"discord.js": "dev",
		"dotenv-flow": "^3.2.0",
		"gm": "^1.23.1",
		"graphql": "^15.5.0",
		"graphql-request": "^3.4.0",
		"i18next": "^20.3.2",
		"lexure": "^0.17.0",
		"metascraper": "^5.22.4",
		"metascraper-image": "^5.22.4",
		"numbro": "^2.3.2",
		"reflect-metadata": "^0.1.13",
		"turndown": "^7.0.0",
		"valid-image-url": "^0.1.4"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^3.1.4",
		"@sapphire/prettier-config": "^1.1.3",
		"@sapphire/ts-config": "^2.2.3",
		"@types/capitalize": "^2",
		"@types/copyfiles": "^2",
		"@types/dotenv-flow": "^3",
		"@types/gm": "^1",
		"@types/i18next": "^13.0.0",
		"@types/metascraper": "^5",
		"@types/metascraper-image": "^5",
		"@types/node": "^15.12.3",
		"@types/rimraf": "^3",
		"@types/turndown": "^5.0.0",
		"copyfiles": "^2.4.1",
		"eslint": "^7.28.0",
		"pm2": "^5.1.0",
		"prettier": "^2.3.1",
		"prisma": "^2.25.0",
		"renovate-config-teal": "^1.0.1",
		"rimraf": "^3.0.2",
		"tsc-alias": "^1.3.3",
		"typescript": "^4.3.4"
	},
	"eslintConfig": {
		"extends": "@sapphire/eslint-config",
		"rules": {
			"@typescript-eslint/no-namespace": "off",
			"@typescript-eslint/no-floating-promises": "off",
			"@typescript-eslint/no-non-null-asserted-optional-chain": "off"
		}
	},
	"prettier": "@sapphire/prettier-config"
}
